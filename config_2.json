{
	"data": {
		"filename": "srikar and zeyang_1.csv",
		"columns": [
			" Assistance Level",
			"Therapist position x",
			"Therapist position y",
			"Therapist position z",
			"Patient position x",
			"Patient position y",
			"Patient position z",
			" Recovery Factor"
		],
		"sequence_length": 5,
		"train_test_split": 0.85,
		"normalise": true
	},
	"training": {
		"epochs": 150,
		"batch_size": 32
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [
			{
				"type": "lstm",
				"neurons": 100,
				"input_timesteps": 4,
				"input_dim": 8,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": true
			},

			{
				"type": "lstm",
				"neurons": 50,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}